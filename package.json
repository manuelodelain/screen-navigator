{
  "name": "screen-navigator",
  "version": "2.0.1",
  "description": "Navigate easily between screens, with transitions.",
  "main": "dist/screen-navigator.js",
  "scripts": {
    "example-basic": "watchify examples/basic/index.js -t babelify -o examples/basic/bundle.js -dv",
    "dev": "browserify src/ScreenNavigator.js -t babelify -o dist/screen-navigator.js",
    "prod": "browserify src/ScreenNavigator.js -t babelify | uglifyjs > dist/screen-navigator.min.js",
    "build": "npm run dev && npm run prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelodelain/screen-navigator.git"
  },
  "keywords": [
    "screen",
    "page",
    "navigator",
    "navigation",
    "animation",
    "transition"
  ],
  "author": "Manuel Odelain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manuelodelain/screen-navigator/issues"
  },
  "homepage": "https://github.com/manuelodelain/screen-navigator",
  "dependencies": {
    "tiny-emitter": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "uglify-js": "^3.4.9",
    "watchify": "^3.2.1"
  }
}
